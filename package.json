{
  "name": "cypress-fun",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "precommit:eslint": "eslint --ignore-path .gitignore . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikementor/cypress-fun.git"
  },
  "author": "mikementor",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mikementor/cypress-fun/issues"
  },
  "homepage": "https://github.com/mikementor/cypress-fun#readme",
  "devDependencies": {
    "cypress": "^3.8.0",
    "eslint": "^6.8.0",
    "eslint-plugin-cypress": "^2.8.1",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "husky": "^3.1.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "scope-enum":[2,"always",["infr"]
    ],
      "subject-case": [
        2,
        "never",
        [
          "start-case",
          "pascal-case"
        ]
      ]
    }
  },
   "husky": {
     "hooks": {
       "pre-commit": "npm run precommit:eslint",
        "commit-msg": "commitlint -E  HUSKY_GIT_PARAMS"
     }
    }
}
